/************************************************** WAYS ********************************************/
way[waterway],
way[waterway][name]                                        { fill-color: gradient(#1a9fa4, #0a3c3e 50%, #000000); builder: water; }

/* roads */
way[highway]                                            { builder:road; width:2; type:pedestrian; }

way[highway=tertiary],
way[highway=residential],
way[highway=service],
way[highway=unclassified],
way[highway=road]                                        { fill-color: gradient(#0fffff, #0bbbbb 50%, #055555); width:9; type:car; }
