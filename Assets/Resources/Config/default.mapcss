canvas {
    behaviours: terrain_draw;

    material_background: vertex_colored;
    material_water: vertex_colored;

    background_color_noise_freq:                            0.1;
    background_ele_noise_freq:                              0.05;

    background_gradient:                                    gradient(#dcdcdc 0%, #c0c0c0 10%, #a9a9a9 50%, #808080);
    water_color_noise_freq:                                 0.1;
    water_ele_noise_freq:                                   0.15;
    water_gradient:                                         gradient(#87cefa, #1e90ff);
    water_surface_level:                                    1;
    water_bottom_level:                                     3;

    car_color_noise_freq:                                   0.1;
    car_ele_noise_freq:                                     0.0;
    car_gradient:                                           gradient(#4d4d4d, #404040);

    pedestrian_color_noise_freq:                            0.1;
    pedestrian_ele_noise_freq:                              0.1;
    pedestrian_gradient:                                    gradient(#636363, #4d4d4d);
}
/* flat areas, general rules */
area[amenity],
area[natural],
area[historic=memorial],
area[landuse]                                            { builder: surface; fill-color: gradient(#10ff00, #0ff000 50%, #098a00); }

