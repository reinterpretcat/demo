way[highway=trunk],
way[highway=trunk_link],
way[highway=primary],
way[highway=primary_link],
way[highway=motorway],
way[highway=motorway_link]                                { fill-color: gradient(#18d2f9, #0e7c93 50%, #04262d); width:8; type:car; }



way[highway=secondary],
way[highway=secondary_link]                                { fill-color: gradient(#0e4eff, #0a37b4 50%, #04184e); width:7; type:car; }

way[highway=tertiary],
way[highway=tertiary_link]                                { fill-color: gradient(#18d2f9, #0e7c93 50%, #04262d); width:8; type:car; }


way[highway=path][foot=designated]                        { fill-color: gradient(#17ff0b, #0fa807 50%, #064203); width:2; type:pedestrian; }

way[highway=service]                                    { fill-color: gradient(#0fffff, #0fffff 50%, #099999); width:1.5; type:pedestrian; }

way[highway=footway]                                     { width:2; type:pedestrian; }

way|z19[barrier]                                            { builder: barrier; fill-color: gradient(#a0a0a0, #808080); material: vertex_colored; height:3; color_noise_freq: 0.3;  }

way|z19[height]                                                { height: eval(num(tag('height'))); }

